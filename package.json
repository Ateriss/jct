    "moment": "^2.30.1"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/lowdb": "^1.0.15",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "esbuild": "0.25.0",
